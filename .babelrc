{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    [
      "module-resolver",
      {
        "root": [
          "./visual"
        ],
        "alias": {
          "~": "./visual",
          "Settings": "./visual/modules/gameSettings.ts",
          "Consts": "./visual/consts/consts.ts"
        }
      }
    ]
  ]
}